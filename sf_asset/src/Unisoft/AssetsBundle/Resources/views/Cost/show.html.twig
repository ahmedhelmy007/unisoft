<h1>Cost</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Costcenterno</th>
            <td>{{ entity.costCenterNo }}</td>
        </tr>
        <tr>
            <th>Arname</th>
            <td>{{ entity.arName }}</td>
        </tr>
        <tr>
            <th>Enname</th>
            <td>{{ entity.enName }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('Cost') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('Cost_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('Cost_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
