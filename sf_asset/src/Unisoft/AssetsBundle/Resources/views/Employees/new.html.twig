{% extends "UnisoftAssetsBundle::layout.html.twig" %}
{% block title %}Assets - Employees list{% endblock %}

{% block content %}
\
{% for flashMessage in app.session.flashbag.get('notice2') %}
            <div  class="alert alert-error">
		<button type="button" class="close" data-dismiss="alert">&times;</button>
		{{ flashMessage }}
	   </div>
        
{% endfor %} 
<div class="row-fluid">
    <div class="span12">
        <div class="box">
            <div class="tab-header">{% trans %}Create new Employee {% endtrans %}</div>
         <div class="row-fluid">
                    <div class="span6">
                        <div class="padded">
                           <div class="row-fluid">
                                <div class="span12">   
<form action="{{ path('Employees_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <button type="submit" class="button blue">{% trans %}Create{% endtrans %}</button>
         <a href="{{ path('Manufacturers') }}" class="button">{% trans %}Back to the list{% endtrans %}</a>
  </form>
              </div>
               </div>
                 </div>
                    </div>
                </div>
        </div>
    </div>
</div>

       
{% endblock %}