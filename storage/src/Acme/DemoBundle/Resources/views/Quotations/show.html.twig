        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('agreements') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('agreements_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('agreements_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
