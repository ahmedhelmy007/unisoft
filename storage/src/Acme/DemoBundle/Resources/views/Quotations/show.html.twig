        </tr>
        <tr>
            <th>Enddate</th>
            <td>{{ entity.endDate|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Evacuationdate</th>
            <td>{{ entity.evacuationDate|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Totalarea</th>
            <td>{{ entity.totalArea }}</td>
        </tr>
        <tr>
