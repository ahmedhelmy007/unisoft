        <tr>
            <th>Depositvalue</th>
            <td>{{ entity.depositValue }}</td>
        </tr>
        <tr>
            <th>Otherfees</th>
            <td>{{ entity.otherFees }}</td>
        </tr>
        <tr>
            <th>Totalvalue</th>
            <td>{{ entity.totalValue }}</td>
        </tr>
        <tr>
            <th>Offerdiscount</th>
            <td>{{ entity.offerDiscount }}</td>
