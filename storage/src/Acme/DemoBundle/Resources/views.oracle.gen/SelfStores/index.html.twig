<h1>SelfStores list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Created</th>
            <th>Hieght</th>
            <th>Length</th>
            <th>Modified</th>
            <th>Name</th>
            <th>Roomsize</th>
            <th>Status</th>
            <th>Width</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('stores_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{% if entity.created %}{{ entity.created|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{{ entity.hieght }}</td>
            <td>{{ entity.length }}</td>
            <td>{% if entity.modified %}{{ entity.modified|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{{ entity.name }}</td>
            <td>{{ entity.roomSize }}</td>
            <td>{{ entity.status }}</td>
            <td>{{ entity.width }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('stores_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('stores_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('stores_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
