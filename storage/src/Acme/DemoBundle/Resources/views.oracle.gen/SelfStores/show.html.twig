<h1>SelfStores</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Created</th>
            <td>{{ entity.created|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Hieght</th>
            <td>{{ entity.hieght }}</td>
        </tr>
        <tr>
            <th>Length</th>
            <td>{{ entity.length }}</td>
        </tr>
        <tr>
            <th>Modified</th>
            <td>{{ entity.modified|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Roomsize</th>
            <td>{{ entity.roomSize }}</td>
        </tr>
        <tr>
            <th>Status</th>
            <td>{{ entity.status }}</td>
        </tr>
        <tr>
            <th>Width</th>
            <td>{{ entity.width }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('stores') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('stores_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('stores_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
